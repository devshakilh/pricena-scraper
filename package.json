{
  "name": "pricena-scraper",
  "version": "1.0.0",
  "description": "Web scraper for e-commerce sites in Bangladesh",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node --project tsconfig.json src/server.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "puppeteer": "^24.7.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^8.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.4"
}